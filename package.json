{
  "name": "masm-tasm",
  "displayName": "MASM/TASM",
  "publisher": "xsro",
  "description": "run MASM/TASM assembly in DOSBox 汇编语言开发插件",
  "version": "0.7.0",
  "keywords": [
    "dosbox",
    "16位",
    "汇编",
    "微机原理",
    "tasm",
    "masm",
    "x86"
  ],
  "icon": "dosboxasm.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "badges": [
    {
      "url": "https://github.com/xsro/masm-tasm/raw/main/pics/asm.png",
      "href": "https://github.com/xsro/masm-tasm/wiki/dosbox",
      "description": "some interesting ASM code"
    }
  ],
  "activationEvents": [
    "onCommand:masm-tasm.dosboxhere",
    "onLanguage:assembly",
    "onLanguage:tasm",
    "onLanguage:masm"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "assembly",
        "aliases": [
          "assembly(DOS)"
        ],
        "extensions": [
          ".asm",
          ".inc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "assembly",
        "path": "./snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "assembly",
        "scopeName": "source.asm",
        "path": "./syntaxes/assembly.tmLanguage.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .ASM || resourceExtname == .asm && editorFocus",
          "command": "masm-tasm.openEmulator",
          "group": "0_MASM-TASM@1"
        },
        {
          "when": "resourceExtname == .ASM || resourceExtname == .asm && editorFocus",
          "command": "masm-tasm.runASM",
          "group": "0_MASM-TASM@2"
        },
        {
          "when": "resourceExtname == .ASM || resourceExtname == .asm && editorFocus",
          "command": "masm-tasm.debugASM",
          "group": "0_MASM-TASM@3"
        }
      ]
    },
    "commands": [
      {
        "command": "masm-tasm.openEmulator",
        "title": "%dosasm.editor.openEmu%"
      },
      {
        "command": "masm-tasm.runASM",
        "title": "%dosasm.editor.runAsm%"
      },
      {
        "command": "masm-tasm.debugASM",
        "title": "%dosasm.editor.debugAsm%"
      },
      {
        "command": "masm-tasm.cleanalldiagnose",
        "title": "%dosasm.command.cleanalldianose%"
      },
      {
        "command": "masm-tasm.dosboxhere",
        "title": "%dosasm.command.dosboxhere%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "MASM/TASM",
      "properties": {
        "masmtasm.ASM.toolspath": {
          "type": "string",
          "markdownDescription": "%dosasm.config.toolspath.mddescription%"
        },
        "masmtasm.ASM.MASMorTASM": {
          "type": "string",
          "default": "TASM",
          "description": "%dosasm.config.masmortasm.description%",
          "enum": [
            "TASM",
            "MASM"
          ],
          "enumDescriptions": [
            "%dosasm.config.masmortasm.enum1%",
            "%dosasm.config.masmortasm.enum2%"
          ]
        },
        "masmtasm.ASM.emulator": {
          "type": "string",
          "default": "dosbox",
          "description": "%dosasm.config.emulator.description%",
          "enum": [
            "dosbox",
            "msdos player",
            "auto"
          ],
          "enumDescriptions": [
            "%dosasm.config.emulator.dosbox%",
            "%dosasm.config.emulator.player%",
            "%dosasm.config.emulator.auto%"
          ]
        },
        "masmtasm.ASM.savefirst": {
          "type": "boolean",
          "default": true,
          "description": "%dosasm.config.savefirst%"
        },
        "masmtasm.language.programmaticFeatures": {
          "type": "boolean",
          "default": true,
          "description": "%dosasm.config.PLF%"
        },
        "masmtasm.language.Hover": {
          "type": "boolean",
          "default": true,
          "description": "%dosasm.config.hover%"
        },
        "masmtasm.dosbox.run": {
          "type": "string",
          "default": "pause",
          "description": "%dosasm.config.boxrun.description%",
          "enum": [
            "keep",
            "exit",
            "pause"
          ],
          "enumDescriptions": [
            "%dosasm.config.boxrun.enum1%",
            "%dosasm.config.boxrun.enum2%",
            "%dosasm.config.boxrun.enum3%"
          ]
        },
        "masmtasm.dosbox.config": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "default": {
            "SDL.windowresolution": "1024x640",
            "SDL.output": "opengl"
          },
          "markdownDescription": "%dosasm.config.boxconfig.description%"
        },
        "masmtasm.dosbox.command": {
          "type": "string",
          "description": "%dosasm.config.boxcommand%"
        },
        "masmtasm.dosbox.console": {
          "type": "string",
          "default": "redirect",
          "description": "%dosasm.config.boxconsole.description%",
          "enum": [
            "min",
            "normal",
            "noconsole",
            "redirect(show)",
            "redirect"
          ],
          "markdownEnumDescriptions": [
            "%dosasm.config.boxconsole.min%",
            "%dosasm.config.boxconsole.normal%",
            "%dosasm.config.boxconsole.noconsole%",
            "%dosasm.config.boxconsole.redirect-show%",
            "%dosasm.config.boxconsole.redirect%"
          ]
        }
      }
    },
    "problemMatchers": [
      {
        "owner": "MASM",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^\\s*(.*)\\((\\d+)\\):\\s+(error|warning)\\s+([A-Z]\\d+:\\s+.*)$",
            "file": 1,
            "line": 2,
            "severity": 3,
            "message": 4,
            "loop": true
          }
        ]
      },
      {
        "owner": "TASM",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^\\s*\\*+(Error|Warning)\\*+\\s+(.*)\\((\\d+)\\)\\s+(.*)$",
            "line": 3,
            "severity": 1,
            "message": 4,
            "file": 2,
            "loop": true
          }
        ]
      }
    ]
  },
  "scripts": {
    "compile": "npx gulp translations-generate && webpack --mode production --vscode-nls",
    "compile-tsc": "tsc -p ./",
    "compile-dev": "webpack --mode development",
    "compile-nls": "npx gulp translations-generate && webpack --mode development --vscode-nls",
    "lint": "eslint src --ext ts",
    "lint-fix": "eslint src --ext ts --fix",
    "watch": "tsc -watch -p ./ --rootDir src",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./dist/test/runTest.js",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.38",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "del": "^4.1.1",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "mocha": "^7.2.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.7",
    "vsce": "^1.83.0",
    "vscode-nls": "^4.1.1",
    "vscode-nls-dev": "^3.2.6",
    "vscode-test": "^1.4.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xsro/masm-tasm"
  },
  "bugs": {
    "url": "https://github.com/xsro/masm-tasm/issues",
    "email": "xsro@foxmail.com"
  },
  "license": "MIT"
}